static int[] icecreamParlor(int m, int[] arr) {
        int[] Index=new int[2];
        for(int i=0;i<arr.length-1;i++){
            for(int j=i+1;j<arr.length;j++){
                if(m==arr[i]+arr[j]){
                    Index[0]=i+1;
                    Index[1]=j+1;
                }
            }
        }
        return Index;

    }
