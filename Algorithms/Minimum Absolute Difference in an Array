 static int minimumAbsoluteDifference(int[] arr) {
        Arrays.sort(arr);
        int min=999999;
        for(int i=0;i<arr.length-1;i++){
            if(Math.abs(arr[i]-arr[i+1])<min){
                    min=Math.abs(arr[i]-arr[i+1]);
                }
            }
        
        return min;
    }
